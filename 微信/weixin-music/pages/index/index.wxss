/**index.wxss**/
.header{
    width:100%;
    height:100rpx;
    background: #222;
    color: #fff;
    text-align: center;
    line-height: 100rpx;
}
.songList{
    flex:1;
    width: 100%;
    padding: 0 40rpx;
    box-sizing: border-box;
}
.song{
    width:100%;
    display: flex;
    border-bottom: 0.5px solid #ccc;
    padding: 20rpx 0;
}
.song image{
    width:200rpx;
    height:200rpx;
    border-left:10rpx solid greenyellow;
}
.tit{
    display: flex;
    flex-direction: column;
    margin-left:40rpx; 
}
.tit text{
    line-height: 100rpx;
}
.tit text:first-child{
    font-size: 36rpx;
    color: #222
}
.tit text:last-child{
    font-size: 28rpx;
    color: gray;
}
